{
  "name": "@qubic-labs/sdk",
  "version": "1.3.0",
  "module": "./dist/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qubickit/jskit-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/qubickit/jskit-sdk/issues"
  },
  "homepage": "https://github.com/qubickit/jskit-sdk#readme",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "check": "biome check .",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "test": "bun test",
    "examples:transfer": "bun run examples/transfer.ts",
    "examples:send-many": "bun run examples/send-many.ts",
    "examples:log-stream": "bun run examples/log-stream.ts",
    "examples:qbi-query": "bun run examples/qbi-query.ts",
    "examples:vault-basic": "bun run examples/vault-basic.ts",
    "release": "bunx semantic-release",
    "prepack": "bun run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/bun": "latest",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "semantic-release": "^24.2.9",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@qubic-labs/core": "^1.0.2",
    "@noble/hashes": "^1.8.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "qubic-vault": "./dist/src/vault-cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "docs/",
    "examples/",
    "CHANGELOG.md",
    "README.md"
  ]
}
